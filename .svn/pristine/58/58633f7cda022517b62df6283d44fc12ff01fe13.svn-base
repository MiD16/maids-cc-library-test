package com.template.Model.Table.AllIndexes;

import java.io.Serializable;
import javax.persistence.*;

import com.fasterxml.jackson.annotation.JsonIgnore;


import java.util.Date;
import lombok.AccessLevel;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;
import lombok.ToString;


/**
 * The persistent class for the IND_RELATIONSHIP_TYPE database table.
 * 
 */
@Entity
@Table(name="IND_RELATIONSHIP_TYPE")
@Getter(AccessLevel.PUBLIC)
@Setter(AccessLevel.PUBLIC)
@NoArgsConstructor
@ToString
public class IndRelationshipType  {

	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	private Long id;

	@Column(name="CB_CODE")
	private String cbCode;

	@Column(name="DELETED_BY")
	private Integer deletedBy;

	@Temporal(TemporalType.DATE)
	@Column(name="DELETED_DATE")
	private Date deletedDate;

	@Column(name="INSERT_BY")
	private Integer insertBy;

	@Temporal(TemporalType.DATE)
	@Column(name="INSERT_DATE")
	private Date insertDate;

	@Column(name="IS_DELETED")
	private Integer isDeleted;

	@Column(name="IS_ENABLED_FLAG")
	private Integer isEnabledFlag;

	@Column(name="LAST_UPDATE_BY")
	private Integer lastUpdateBy;

	@Temporal(TemporalType.DATE)
	@Column(name="LAST_UPDATE_DATE")
	private Date lastUpdateDate;

	@Column(name="NAME_A")
	private String nameA;

	@Column(name="NAME_B")
	private String nameB;

	private String note;

	@Column(name="PLACE_ORDER")
	private Integer placeOrder;

	@PrePersist
	  void createdAt() {
	    this.insertDate = this.lastUpdateDate = new Date();
	    this.isDeleted = 0;
	  }
	  
	  @PreUpdate
	  protected void onUpdate() {
	    this.lastUpdateDate = new Date();
	  }

}