	}
	
	.animate-element-before {


    -webkit-transition: all 0.5s;
    -moz-transition: all 0.5;
    -o-transition: all 0.5;
    transition: all 0.5s;
  }
  
  .animate-element-after {
   visibility: visible;
    opacity: 1; 
  }
.container-div {
  display: none;
  opacity: 0;
  transition: opacity 0.2s ease;
 
}

.container-div.show {
  display:flex;
}
 
.container-div.active {
  opacity: 1;
}


