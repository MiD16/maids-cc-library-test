.btn-primary-red {
  color: #ffffff;
  
  background-image: linear-gradient(to bottom, #c60606, #320505);
  border-color: rgb(198, 6, 6);
   }
  .btn-primary-red:hover {
    color: #ffffff;
    background-color: rgb(249, 38, 38);
    border-color: rgb(198, 6, 6); 
    }
  .btn-primary-red:focus, .btn-primary-red.focus {
    color: #ffffff;
    background-color: rgb(198, 6, 6);
    border-color: rgb(194, 6, 6);
    box-shadow: 0 0 0 0.2rem rgb(190, 6, 6,0.5); 
    
    }

.btn-primary-green {
  color: #ffffff;
  background-color: rgb(33, 107, 35);
  border-color: rgb(33, 107, 35);
   }
  .btn-primary-green:hover {
    color: #ffffff;
    background-color: rgb(46, 150, 48);
    border-color: rgb(33, 107, 35); 
    }
  .btn-primary-green:focus, .btn-primary-green.focus {
    color: #ffffff;
    background-color: rgb(33, 107, 35);
    border-color: rgb(33, 107, 35);
    box-shadow: 0 0 0 0.2rem rgb(33, 104, 35,0.5); 
    
    }
.fontstyle{

font-family: "Calibri", sans-serif !important;
 }
.dropdown-item-table {
  display: block;
  width: 80%;
  padding: 0.25rem 1.5rem;
  clear: both;
  font-weight: 400;
  color: #00008b;
  text-align: left;
  white-space: nowrap;
  background-color: transparent;
  border: 0; }
  .dropdown-item-table:hover, .dropdown-item-table:focus {
    color: #16181b;
    text-decoration: none;
    width: 80%;
    background-color: #f8f9fa; }
  .dropdown-item-table.active, .dropdown-item-table:active {
    color: #ffffff;
    text-decoration: none;
    background-color: #1b68ff; }
  .dropdown-item-table.disabled, .dropdown-item-table:disabled {
    color: #6c757d;
    pointer-events: none;
    background-color: transparent; }
    
    span.date{
		color:#c50202;
